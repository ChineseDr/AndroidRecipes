<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.raymond.basedemo">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.baseDemo.activities.FirstActivity" />
        <activity android:name="com.baseDemo.activities.SecondActivity" />
        <activity android:name="com.baseDemo.activities.ThirdActivity" />
        <activity android:name="com.baseDemo.activities.FourthActivity" />

        <service
            android:name="com.baseDemo.services.MyService"
            android:enabled="true"
            android:exported="true" />
        <service
            android:name="com.baseDemo.services.MyIntentService"
            android:exported="false" />

        <receiver
            android:name="com.baseDemo.receivers.MyReceiver"
            android:enabled="true"
            android:exported="true"></receiver>
    </application>

</manifest>